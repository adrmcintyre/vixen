;; Compute sum of squares
;;
;; 0x30 * 0x30 + 0x40 * 0x40 = 0x1900
;;
00 c300 ; ldw r0, #30
02 c301 ; ldw r1, #30
04 fe06 ; bl .mul
06 0023 ; mov r3, r2
08 c400 ; ldw r0, #40
0a c401 ; ldw r1, #40
0c fe02 ; bl .mul
0e 0423 ; add r3, r2
10 ffff ; halt

.mul
12 c002 ; ldw r2, #0
.loop
14 1f00 ; tst r0, #0
16 e001 ; beq .skip
18 0412 ; add r2, r1
.skip
1a 1911 ; lsl r1, #1
1c 1a10 ; lsr r0, #1
1e e1fa ; bne .loop
20 00ef ; mov pc, r14

